{
  "kinesis.endpoint":"kinesis.ap-northeast-2.amazonaws.com",
  "assumeRoleARN":"arn:aws:iam::122797595191:role/svrlog-kinesis-put-role-prod",
  "cloudwatch.emitMetrics":false,
  "log.statusReportingPeriodSeconds":300,
  "flows":[
    {
      "filePattern":"/var/log/messages",
      "kinesisStream":"syslog",
      "maxBufferAgeMillis":1000,
      "partitionKeyOption":"RANDOM",
      "multiLineStartPattern":"^[^ ]+",
      "dataProcessingOptions":[
        {
          "optionName":"ADDEC2METADATA",
          "logType":"messages",
          "withTzOffset":true,
          "metadataTTL":86400,
          "tagFields":[
            "Team",
            "Service",
            "Environment",
            "elasticbeanstalk:environment-id",
            "elasticbeanstalk:environment-name"
          ]
        }
      ]
    },
    {
      "filePattern":"/var/log/secure",
      "kinesisStream":"syslog",
      "maxBufferAgeMillis":1000,
      "partitionKeyOption":"RANDOM",
      "multiLineStartPattern":"^[^ ]+",
      "dataProcessingOptions":[
        {
          "optionName":"ADDEC2METADATA",
          "metadataTTL":86400,
          "withTzOffset":true,
          "logType":"secure",
          "tagFields":[
            "Team",
            "Service",
            "Environment",
            "elasticbeanstalk:environment-id",
            "elasticbeanstalk:environment-name"
          ]
        }
      ]
    },
    {
      "filePattern":"/var/log/commands.log",
      "kinesisStream":"syslog",
      "maxBufferAgeMillis":1000,
      "partitionKeyOption":"RANDOM",
      "multiLineStartPattern":"^[^ ]+",
      "dataProcessingOptions":[
        {
          "optionName":"ADDEC2METADATA",
          "metadataTTL":86400,
          "withTzOffset":true,
          "logType":"commands",
          "tagFields":[
            "Team",
            "Service",
            "Environment",
            "elasticbeanstalk:environment-id",
            "elasticbeanstalk:environment-name"
          ]
        }
      ]
    }
  ]
}
